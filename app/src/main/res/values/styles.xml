<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/windowBack</item>
    </style>

    <style name="dialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@drawable/dialogback</item>
    </style>
    <style name="button" parent="Theme.AppCompat.DayNight">
        <item name="colorButtonNormal">#9973b7</item>
        <item name="android:colorPressedHighlight">#d90000</item>
    </style>
</resources>
